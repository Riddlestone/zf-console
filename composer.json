{
    "name": "clockwork/console",
    "description": "A Zend Framework 2 module for Symfony Console",
    "type": "library",
    "require": {
        "php": "^7.3",
        "psr/container": "^1.0",
        "symfony/console": "^4.3",
        "zendframework/zend-component-installer": "^1.0 || ^0.7",
        "zendframework/zend-mvc": "^3.1",
        "zendframework/zend-servicemanager": "^3.4",
        "zendframework/zend-stdlib": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "Clockwork\\Console\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4",
        "container-interop/container-interop": "^1.2"
    },
    "autoload-dev": {
        "psr-4": {
            "Clockwork\\Console\\Test\\": "test/"
        }
    },
    "extra": {
        "zf": {
            "component": "Clockwork\\Console"
        }
    },
    "bin": [
        "bin/console"
    ],
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Andrew Riddlestone",
            "email": "andrew.riddlestone@gmail.com"
        }
    ]
}
